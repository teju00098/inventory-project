<p id="notice"><%= notice %></p>

<h2 style="text-align:center">Search Results </h2>

<div class="media">
  <div class="media-body">
    <div class="table-wrapper">
        <table class="table table-striped table-hover">
        <thead>
            <tr>
            <th>Supplier Name</th>
            <th>Primary Contact</th>
            <th>Address</th>
            <th>ContactNo</th>
            <th>Email</th>
            <th>GSTNO</th>
            <th>DLNO20B</th>
            <th>DLNO21B</th>
            <th colspan="3"></th>
            </tr>
        </thead>

        <tbody>
            <% @suppliers.each do |supplier| %>
            <tr>
                <td><%= supplier.suppliername %></td>
                <td><%= supplier.primarycontact %></td>
                <td><%= supplier.address %></td>
                <td><%= supplier.contactno %></td>
                <td><%= supplier.email %></td>
                <td><%= supplier.gstno %></td>
                <td><%= supplier.dlno20b %></td>
                <td><%= supplier.dlno21b %></td>
                <td><%= link_to 'Show', supplier, class: 'btn btn-mini btn-info' %></td>
                <td><%= link_to 'Edit', edit_supplier_path(supplier), class: 'btn btn-mini btn-warning' %></td>
                <td><%= link_to 'Destroy', supplier, method: :delete, class: 'btn btn-mini btn-danger', data: { confirm: 'Are you sure?' } %></td>
            </tr>
            <% end %>
        </tbody>
        </table>
    </div>
  </div>
</div>  

<p>
  <%= link_to 'Back to all master data', suppliers_path, class: 'btn btn-mini btn-warning' %>
</p>